
#include<bits/stdc++.h>
using namespace std;
vector<int>v;
int main(){
    int i,n,iteam,x,z;
    cin>>n;
    for(i=0; i<n; i++){
        cin>>x;
        v.push_back(x);
    }
    cin>>iteam;
    auto it= lower_bound(v.begin(),v.end(), iteam);
    z = it-v.begin(); ///index found
    cout<<*it<<endl;
    cout<<z<<endl;
return 0;
}
