#include<bits/stdc++.h>
using namespace std;
main(){
    int n,i;
    string a;
    cin>>n;
    vector<string>v;
    while(n--){
        cin>>a;
        v.push_back(a);
    }
        //len = v.size();
        for(i=0; i<v.size(); i++){
            for(int j=0; j<v[i].size(); j++){
                int x = v[i][j]-'0';
                cout<<x;
            }
            cout<<endl;
        }
}
