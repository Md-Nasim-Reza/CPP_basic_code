#include<bits/stdc++.h>
using namespace std;
main(){
    int i,n,a;
    vector<int>v;
    cin>>n;
    for(i=0; i<n; i++){
        cin>>a;
        v.push_back(a);
    }
    for(i=0; i<v.size(); i++)
        cout<<v[i]<<" ";
    sort(v.begin(),v.end());
    cout << "\nsort: ";
    for(i=0; i<n; i++)
        printf("%d ", v[i]);
    printf("\n");
    v.pop_back();
    printf("pop back: ");
    for(i=0; i<v.size(); i++)
        printf("%d ", v[i]);
    cout<<endl;

return 0;
}
