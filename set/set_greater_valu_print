#include<bits/stdc++.h>
using namespace std;
int main(){
    int i,j,n,m;
    set<int,greater<int> >st;
    set<int>::iterator it;
    cin>>n;
    for(i=0; i<n; i++){
        cin>>m;
        st.insert(m);
    }
    for(it=st.begin(); it!=st.end(); it++){
        cout<<*it<<" ";
    }
   // for(auto it:st){
      //  cout<<it<<" ";
   // }
    cout<<endl;
return 0;
}
