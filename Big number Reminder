
#include<bits/stdc++.h>
using namespace std;
int mod(string num,int a)
{
    int res=0;
    for(int i=0;i<(int)num.size(); i++)
         res=(res*10+(int)num[i]-'0')%a;
    return res;
}

int main(){
//    freopen("in.txt","r",stdin);
    int a,b,c,i,j,t,k,lie,m,n,o,x,y,z,ar[200010];
    cin>>n;
    while(n--){
        string s;
        cin>>s;
        cin>>m;
        z=mod(s,m);
        printf("%lld\n",z);
    }

    return 0;
}
